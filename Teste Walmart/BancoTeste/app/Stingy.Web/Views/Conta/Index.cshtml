@model IEnumerable<Stingy.Web.Models.ContaViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Contas</h2>


<table>
    <tr>
        <th>NumeroConta
        </th>
        <th>Agencia
        </th>
        <th>Saldo
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumeroConta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Agencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Saldo)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id = item.Id }) |     
                @Html.ActionLink("Deletar", "Deletar", new { id = item.Id })
            </td>
        </tr>
    }

</table>
